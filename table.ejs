<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tabela</title>

    <style>
        thead {
            background-color: rgb(41, 117, 223);;
            color: white;
            border: 1px solid white;
        }
        td {
            border: 1px inherit rgb(41, 117, 223);;
            color: rgb(41, 117, 223);;
        }
        h1 {
            color: rgb(41, 117, 223);
        }
    </style>

</head>
<body>

    <header>
        <p>LOGO</p>
    </header>

    <h1>Tabela</h1>
    <hr>

    <table>
        <thead>
            <% header.forEach(col => { %>
                <th> <%= col %> </th>
           <% }) %>
        </thead>
        <tbody>
            <% rows.forEach(row => { %>
                <tr>
                    <% row.forEach(element => { %> 
                        <td><%= element %> </td>
                    <%}) %>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>